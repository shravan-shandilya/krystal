#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x95dd580 .scope module, "memory_tb" "memory_tb" 2 1;
 .timescale 0 0;
v0x95da098_0 .var "addr", 7 0;
v0x95da118_0 .var "d_in", 11 0;
v0x95da188_0 .net "d_out", 11 0, v0x95d9fd0_0; 1 drivers
v0x95da1f8_0 .var "rwbar", 0 0;
S_0x95dd600 .scope module, "mem" "memory" 2 22, 3 1, S_0x95dd580;
 .timescale 0 0;
v0x95dd680_0 .net "addr", 7 0, v0x95da098_0; 1 drivers
v0x95d9f20 .array "binary", 255 0, 11 0;
v0x95d9f70_0 .net "d_in", 11 0, v0x95da118_0; 1 drivers
v0x95d9fd0_0 .var "d_out", 11 0;
v0x95da038_0 .net "rwbar", 0 0, v0x95da1f8_0; 1 drivers
E_0x95dd8c0 .event edge, v0x95dd680_0;
E_0x95dda38 .event edge, v0x95da038_0;
    .scope S_0x95dd600;
T_0 ;
    %vpi_call 3 10 "$readmemh", "binary.hex", v0x95d9f20;
    %end;
    .thread T_0;
    .scope S_0x95dd600;
T_1 ;
    %wait E_0x95dda38;
    %load/v 8, v0x95da038_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/getv 3, v0x95dd680_0;
    %load/av 8, v0x95d9f20, 12;
    %set/v v0x95d9fd0_0, 8, 12;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v0x95d9f70_0, 12;
    %ix/getv 3, v0x95dd680_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x95d9f20, 8, 12;
t_0 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x95dd600;
T_2 ;
    %wait E_0x95dd8c0;
    %load/v 8, v0x95da038_0, 1;
    %jmp/0xz  T_2.0, 8;
    %ix/getv 3, v0x95dd680_0;
    %load/av 8, v0x95d9f20, 12;
    %set/v v0x95d9fd0_0, 8, 12;
    %jmp T_2.1;
T_2.0 ;
    %load/v 8, v0x95d9f70_0, 12;
    %ix/getv 3, v0x95dd680_0;
   %jmp/1 t_1, 4;
   %ix/load 1, 0, 0;
   %set/av v0x95d9f20, 8, 12;
t_1 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x95dd580;
T_3 ;
    %set/v v0x95da098_0, 0, 8;
    %set/v v0x95da1f8_0, 1, 1;
    %vpi_call 2 12 "$monitor", "%d\011%d\011r!w=%b,d_out=%b,d_in=%b", $time, v0x95da098_0, v0x95da1f8_0, v0x95da188_0, v0x95da118_0;
    %vpi_call 2 13 "$dumpfile", "plot.vcd";
    %end;
    .thread T_3;
    .scope S_0x95dd580;
T_4 ;
    %delay 5, 0;
    %load/v 8, v0x95da098_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %set/v v0x95da098_0, 8, 8;
    %load/v 8, v0x95da098_0, 8;
    %cmpi/u 8, 255, 8;
    %jmp/0xz  T_4.0, 4;
    %vpi_call 2 19 "$finish";
T_4.0 ;
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "memory_tb.v";
    "memory.v";
